<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.ContactoActivity">

    <include android:id="@+id/action_bar"
        layout="@layout/action_bar" />

    <TextView android:id="@+id/tv_bio_desarrollador"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:textSize="@dimen/bio_desarrollador_text_size"
        android:text="@string/bio_desarrollador_text"
        android:layout_below="@id/action_bar"
        android:gravity="center"/>

</RelativeLayout>